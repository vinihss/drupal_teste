{{ attach_library('spa_manager/spamanager-slider') }}

<ul id="lightSlider">
    {% for row in rows %}
        <li>
            <a href="{{ row.link }}">
                <img style="width:100%" src="{{ row.imagem }}" >
            </a>
            <h1>{{ row.titulo }}</h1>
            <p>{{ row.texto }}</p>
        </li>
    {% endfor %}
</ul>